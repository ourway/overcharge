        <%= if @page_type == "product" do %>
            <i class="fa fa-angle-double-left grey_color"></i>
            <li itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem"> 
              <a itemscope itemtype="http://schema.org/Thing" 
              itemprop="item" class="page-link <%= @product %>_color" 
              href="<%= @conn.request_path %>" > <span itemprop="name"><%= @title %></span></a>
              <meta itemprop="position" content="3" />
            </li>
        <% end %>
        </ol>
    <% end %>

      
      
    
  </div>
</header>
<div class="subtitle">
 <h1> <%= @subtitle %> </h1>
</div>
    <main class="content" role="main">

            <%= render @view_module, @view_template, assigns %>

  <nav class="pagination" role="navigation">
    
    <span class="page-number"></span>
    
  </nav>

</main>


  <%= render Overcharge.LayoutView, "footer.html", assigns %>



  </body>
</html>

